{
  "name": "neonbinder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "npm run predev && next dev",
    "dev:backend": "./dev-backend.sh",
    "env:browser": "npx convex env set NEONBINDER_BROWSER_URL",
    "tunnel": "npx localtunnel --port 8080",
    "predev": "./setup-env.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "builder": "npx builder.io@latest launch --app --serverUrl http://localhost:3000",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@clerk/backend": "^2.19.0",
    "@clerk/nextjs": "^6.34.0",
    "@convex-dev/auth": "^0.0.81",
    "@google-cloud/secret-manager": "^6.0.1",
    "@google-cloud/storage": "^7.17.2",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/themes": "^3.2.1",
    "@sentry/nextjs": "^10.22.0",
    "@types/crypto-js": "^4.2.2",
    "axios": "^1.10.0",
    "convex": "^1.27.1",
    "convex-helpers": "^0.1.104",
    "crypto-js": "^4.2.0",
    "next": "^15.2.8",
    "playwright": "^1.53.1",
    "posthog-js": "^1.280.1",
    "posthog-node": "^5.10.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "husky": "^9.1.7",
    "localtunnel": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
